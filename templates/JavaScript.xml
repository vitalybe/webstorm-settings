<templateSet group="JavaScript">
  <template name="lc" value="$variable$loggerCreator(&quot;$method$&quot;, moduleLogger);" description="logging" toReformat="true" toShortenFQNames="true">
    <variable name="variable" expression="" defaultValue="&quot;let logger = &quot;" alwaysStopAt="true" />
    <variable name="method" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JSX_HTML" value="true" />
      <option name="JS_STATEMENT" value="true" />
    </context>
  </template>
  <template name="l" value="logger.info(`$LOG$`);" description="log" toReformat="false" toShortenFQNames="true">
    <variable name="LOG" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JS_STATEMENT" value="true" />
    </context>
  </template>
  <template name="lm" value="import loggerCreator from '../utils/logger'&#10;var moduleLogger = loggerCreator(&quot;$FILE$&quot;);&#10;" description="logging module" toReformat="false" toShortenFQNames="true">
    <variable name="FILE" expression="fileNameWithoutExtension()" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JAVA_SCRIPT" value="true" />
    </context>
  </template>
</templateSet>